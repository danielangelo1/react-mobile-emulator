{
  "version": "0.0.0-development",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "babel src -d dist",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "deploy": "npm version patch && npm publish",
    "semantic-release": "semantic-release --branches main"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "react-mobile-emulator",
  "author": "danielangelo1",
  "module": "dist/react-mobile-emulator.esm.js",
  "size-limit": [
    {
      "path": "dist/react-mobile-emulator.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-mobile-emulator.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/cli": "^7.27.0",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "husky": "^9.1.7",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "size-limit": "^11.2.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "semantic-release": "^24.2.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danielangelo1/react-mobile-emulator.git"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
